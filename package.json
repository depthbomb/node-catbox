{
	"name": "node-catbox",
	"version": "2.1.0",
	"author": "depthbomb",
	"description": "A library for interacting with Catbox.moe written in TypeScript.",
	"license": "MIT",
	"keywords": [
		"catbox.moe",
		"catbox",
		"typescript",
		"fetch",
		"api"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/node-catbox.git"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/node-catbox/issues"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"start": "node .",
		"prebuild": "rimraf ./dist/*",
		"build": "tsc",
		"dist": "tsup",
		"prerelease": "yarn dist",
		"release": "release-it",
		"lint": "eslint ./src --ext .ts"
	},
	"dependencies": {
		"form-data": "^4.0.0",
		"node-fetch-native": "^0.1.3"
	},
	"devDependencies": {
		"@swc/core": "^1.2.187",
		"@tsconfig/recommended": "^1.0.1",
		"@types/eslint": "^8.4.2",
		"@types/node": "^17.0.35",
		"@types/rimraf": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"eslint": "^8.16.0",
		"release-it": "^15.0.0",
		"rimraf": "^3.0.2",
		"tslib": "^2.4.0",
		"tsup": "^5.12.8",
		"typescript": "^4.6.4"
	},
	"packageManager": "yarn@3.2.1"
}
