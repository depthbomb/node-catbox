{
	"name": "node-catbox",
	"version": "0.1.0",
	"author": "depthbomb",
	"description": "A library for interacting with Catbox.moe written in TypeScript.",
	"keywords": [
		"catbox.moe",
		"catbox",
		"typescript",
		"fetch",
		"api"
	],
	"license": "GPLv3",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/node-catbox.git"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/node-catbox/issues"
	},
	"files": [
		"dist",
		"dist/**/*.d.ts",
		"dist/**/*.d.ts.map"
	],
	"scripts": {
		"start": "node .",
		"prebuild": "rimraf ./dist/*",
		"build": "tsc",
		"predist": "npm run prebuild",
		"dist": "tsup",
		"prerelease": "npm run dist",
		"release": "release-it",
		"lint": "eslint ./src --ext .ts"
	},
	"dependencies": {
		"form-data": "^4.0.0",
		"node-fetch": "^2.6.7",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@swc/core": "^1.2.139",
		"@tsconfig/recommended": "^1.0.1",
		"@types/eslint": "^8.4.1",
		"@types/node": "^17.0.18",
		"@types/node-fetch": "2.6.0",
		"@types/rimraf": "^3.0.2",
		"@types/source-map-support": "^0.5.4",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"eslint": "^8.9.0",
		"release-it": "^14.12.4",
		"rimraf": "^3.0.2",
		"tsup": "^5.11.13",
		"typescript": "^4.5.5"
	}
}
